{
  "name": "tabgrouper",
  "displayName": "Tabgrouper",
  "version": "0.0.1",
  "description": "Modern tab management.",
  "author": "Cody Williamson (cody@spectaclewebdesign.com)",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@plasmohq/messaging": "0.6.2",
    "@plasmohq/storage": "1.9.3",
    "@supabase/supabase-js": "2.39.3",
    "plasmo": "0.85.2",
    "psl": "^1.9.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.3",
    "typescript": "5.3.3"
  },
  "manifest": {
    "key": "$CRX_PUBKEY",
    "permissions": [
      "tabs",
      "tabGroups",
      "storage"
    ],
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "options.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}